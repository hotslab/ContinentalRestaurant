{
  "compilerOptions": {
    "target": "ES2015",
    "lib": [
      "es2015.promise"
    ],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": false
  },
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Recommended"
}